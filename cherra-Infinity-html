    <script>
        // ÿ•ŸÜÿ¥ÿßÿ° ÿßŸÑŸÜÿ¨ŸàŸÖ
        function createStars() {
            const starsContainer = document.getElementById('starsContainer');
            for (let i = 0; i < 200; i++) {
                const star = document.createElement('div');
                star.className = 'star';
                star.style.width = Math.random() * 3 + 'px';
                star.style.height = star.style.width;
                star.style.left = Math.random() * 100 + '%';
                star.style.top = Math.random() * 100 + '%';
                star.style.animationDelay = Math.random() * 3 + 's';
                starsContainer.appendChild(star);
            }
        }

        // ŸÜÿ∏ÿßŸÖ ÿßŸÑÿ•ÿ¥ÿπÿßÿ±ÿßÿ™
        function showNotification(message, type = 'success') {
            const notifications = document.getElementById('notifications');
            const notification = document.createElement('div');
            notification.className = `notification ${type}`;
            notification.textContent = message;
            notifications.appendChild(notification);
            
            // ÿ•ÿ∏Ÿáÿßÿ± ÿßŸÑÿ•ÿ¥ÿπÿßÿ±
            setTimeout(() => {
                notification.classList.add('show');
            }, 100);
            
            // ÿ•ÿÆŸÅÿßÿ° ÿßŸÑÿ•ÿ¥ÿπÿßÿ± ÿ®ÿπÿØ 5 ÿ´ŸàÿßŸÜ
            setTimeout(() => {
                notification.classList.remove('show');
                setTimeout(() => {
                    notifications.removeChild(notification);
                }, 300);
            }, 5000);
        }

        // ŸÜÿ∏ÿßŸÖ ÿ®ÿµŸÖÿ© ÿßŸÑÿ•ÿµÿ®ÿπ
        function showFingerprintScanner() {
            document.getElementById('fingerprintSection').style.display = 'block';
            document.getElementById('mainGate').style.display = 'none';
        }

        function hideFingerprintScanner() {
            document.getElementById('fingerprintSection').style.display = 'none';
            document.getElementById('mainGate').style.display = 'block';
            resetFingerprintScanner();
        }

        function resetFingerprintScanner() {
            const scanner = document.getElementById('fingerprintScanner');
            const status = document.getElementById('fingerprintStatus');
            
            scanner.classList.remove('scanning', 'success');
            scanner.style.transform = 'scale(1)';
            status.textContent = 'ÿßÿ∂ÿ∫ÿ∑ ÿπŸÑŸâ ÿßŸÑŸÖÿßÿ≥ÿ≠ ÿßŸÑÿ∂Ÿàÿ¶Ÿä ŸÑŸÑÿØÿÆŸàŸÑ ÿ•ŸÑŸâ ÿßŸÑŸÖŸÜÿµÿ©';
            status.style.color = 'var(--neon-blue)';
        }

        const fingerprintScanner = document.getElementById('fingerprintScanner');
        const fingerprintStatus = document.getElementById('fingerprintStatus');

        fingerprintScanner.addEventListener('click', function() {
            if (this.classList.contains('success')) return;
            
            this.classList.add('scanning');
            fingerprintStatus.textContent = 'üîç ÿ¨ÿßÿ±Ÿä ŸÖÿ≥ÿ≠ ÿßŸÑÿ®ÿµŸÖÿ©...';
            fingerprintStatus.style.color = 'var(--neon-orange)';
            
            // ŸÖÿ≠ÿßŸÉÿßÿ© ÿßŸÑŸÖÿ≥ÿ≠
            setTimeout(() => {
                this.classList.remove('scanning');
                this.classList.add('success');
                fingerprintStatus.innerHTML = '‚úÖ ÿ™ŸÖ ÿßŸÑÿ™ÿπÿ±ŸÅ ÿπŸÑŸâ ÿßŸÑÿ®ÿµŸÖÿ© ÿ®ŸÜÿ¨ÿßÿ≠!<br>ÿ¨ÿßÿ±Ÿä ÿßŸÑÿØÿÆŸàŸÑ ÿ•ŸÑŸâ ÿßŸÑŸÖŸÜÿµÿ©...';
                fingerprintStatus.style.color = 'var(--neon-green)';
                
                // ŸÖÿ≠ÿßŸÉÿßÿ© ÿßŸÑÿØÿÆŸàŸÑ ÿ•ŸÑŸâ ÿßŸÑŸÖŸÜÿµÿ©
                setTimeout(() => {
                    showNotification('üéâ ÿ™ŸÖ ÿßŸÑÿØÿÆŸàŸÑ ÿ®ŸÜÿ¨ÿßÿ≠ ÿ®ÿßÿ≥ÿ™ÿÆÿØÿßŸÖ ÿ®ÿµŸÖÿ© ÿßŸÑÿ•ÿµÿ®ÿπ!', 'success');
                    showDashboard();
                }, 2000);
            }, 3000);
        });

        function showPasswordLogin() {
            const password = prompt('üîê ÿ£ÿØÿÆŸÑ ŸÉŸÑŸÖÿ© ÿßŸÑŸÖÿ±Ÿàÿ± ŸÑŸÑÿØÿÆŸàŸÑ:', 'Bismillah');
            if (password === 'Bismillah') {
                showNotification('üéâ ÿ™ŸÖ ÿßŸÑÿØÿÆŸàŸÑ ÿ®ŸÜÿ¨ÿßÿ≠ ÿ®ÿßÿ≥ÿ™ÿÆÿØÿßŸÖ ŸÉŸÑŸÖÿ© ÿßŸÑŸÖÿ±Ÿàÿ±!', 'success');
                showDashboard();
            } else {
                showNotification('‚ùå ŸÉŸÑŸÖÿ© ÿßŸÑŸÖÿ±Ÿàÿ± ÿ∫Ÿäÿ± ÿµÿ≠Ÿäÿ≠ÿ©', 'error');
            }
        }

        function showFaceRecognition() {
            showNotification('üëÅÔ∏è ŸÜÿ∏ÿßŸÖ ÿßŸÑÿ™ÿπÿ±ŸÅ ÿπŸÑŸâ ÿßŸÑŸàÿ¨Ÿá ŸÇŸäÿØ ÿßŸÑÿ™ÿ∑ŸàŸäÿ±', 'warning');
        }

        function showDashboard() {
            document.getElementById('mainGate').style.display = 'none';
            document.getElementById('dashboard').style.display = 'block';
            document.getElementById('fingerprintSection').style.display = 'none';
            
            // ÿπÿ±ÿ∂ ÿ•ÿ¥ÿπÿßÿ± ÿ™ÿ±ÿ≠Ÿäÿ®Ÿä
            setTimeout(() => {
                showNotification('üöÄ ŸÖÿ±ÿ≠ÿ®Ÿãÿß ŸÅŸä ŸÑŸàÿ≠ÿ© ÿ™ÿ≠ŸÉŸÖ Cherrak Infinity! ÿ™ŸÖ ÿ™ŸÅÿπŸäŸÑ ÿ¨ŸÖŸäÿπ ÿßŸÑÿ£ŸÜÿ∏ŸÖÿ©.', 'success');
            }, 500);
        }

        function showMainGate() {
            document.getElementById('dashboard').style.display = 'none';
            document.getElementById('mainGate').style.display = 'block';
        }

        // ŸÜÿ∏ÿßŸÖ ÿßŸÑÿ≥ÿßÿπÿ© ÿßŸÑŸÉŸàŸÜŸäÿ©
        function updateCosmicClock() {
            const now = new Date();
            const cosmicClock = document.getElementById('cosmicClock');
            const timeAlgeria = document.getElementById('timeAlgeria');
            const timeKSA = document.getElementById('timeKSA');
            const timeUAE = document.getElementById('timeUAE');
            const timeUTC = document.getElementById('timeUTC');

            cosmicClock.textContent = now.toLocaleTimeString('ar-EG');
            
            // ÿ£ŸàŸÇÿßÿ™ ÿßŸÑÿØŸàŸÑ
            const options = { timeZone: 'Africa/Algiers', hour12: true, hour: '2-digit', minute: '2-digit', second: '2-digit' };
            timeAlgeria.textContent = now.toLocaleTimeString('ar-EG', options);
            
            options.timeZone = 'Asia/Riyadh';
            timeKSA.textContent = now.toLocaleTimeString('ar-EG', options);
            
            options.timeZone = 'Asia/Dubai';
            timeUAE.textContent = now.toLocaleTimeString('ar-EG', options);
            
            options.timeZone = 'UTC';
            timeUTC.textContent = now.toLocaleTimeString('ar-EG', options);
        }

        // ŸÜÿ∏ÿßŸÖ ÿßŸÑÿ£ŸÇŸÖÿßÿ± ÿßŸÑÿµŸÜÿßÿπŸäÿ©
        function updateNetworkSpeeds() {
            const speed5g = document.getElementById('speed5g');
            const speed6g = document.getElementById('speed6g');
            const latency = document.getElementById('latency');
            const networkSpeed = document.getElementById('networkSpeed');
            const speedIndex = document.getElementById('speedIndex');

            // ŸÖÿ≠ÿßŸÉÿßÿ© ÿ≥ÿ±ÿπÿßÿ™ ÿπÿ¥Ÿàÿßÿ¶Ÿäÿ© ŸàÿßŸÇÿπŸäÿ©
            const speed5gValue = 850 + Math.random() * 150;
            const speed6gValue = 3200 + Math.random() * 800;
            const latencyValue = 8 + Math.random() * 4;

            speed5g.textContent = Math.round(speed5gValue) + ' Mbps';
            speed6g.textContent = (speed6gValue / 1000).toFixed(1) + ' Gbps';
            latency.textContent = latencyValue.toFixed(1) + ' ms';
            networkSpeed.textContent = (speed6gValue / 1000).toFixed(1) + ' G';
            speedIndex.textContent = (speed6gValue / 1000).toFixed(1) + 'G';
        }

        // ŸÜÿ∏ÿßŸÖ ÿßŸÑÿ¥ÿ≠ŸÜ ÿßŸÑÿ¥ŸÖÿ≥Ÿä
        function updateBatteryStatus() {
            const batteryLevel = document.getElementById('batteryLevel');
            const chargeStatus = document.getElementById('chargeStatus');
            const chargePercentage = document.getElementById('chargePercentage');

            // ŸÖÿ≠ÿßŸÉÿßÿ© ÿ≠ÿßŸÑÿ© ÿßŸÑÿ¥ÿ≠ŸÜ
            const chargeLevel = 75 + Math.random() * 10;
            batteryLevel.style.width = chargeLevel + '%';
            chargePercentage.textContent = Math.round(chargeLevel) + '%';

            if (chargeLevel > 90) {
                chargeStatus.textContent = 'üîã ŸÖÿ¥ÿ≠ŸàŸÜ ÿ®ÿßŸÑŸÉÿßŸÖŸÑ';
                chargeStatus.style.color = 'var(--neon-green)';
            } else {
                chargeStatus.textContent = '‚òÄÔ∏è ÿ¨ÿßÿ±Ÿä ÿßŸÑÿ¥ÿ≠ŸÜ ÿ®ÿßŸÑÿ∑ÿßŸÇÿ© ÿßŸÑÿ¥ŸÖÿ≥Ÿäÿ©';
                chargeStatus.style.color = 'var(--neon-yellow)';
            }
        }

        // ŸÜÿ∏ÿßŸÖ ÿßŸÑÿ•ÿ≠ÿµÿßÿ¶Ÿäÿßÿ™
        function updateStats() {
            const onlineUsers = document.getElementById('onlineUsers');
            const totalEarnings = document.getElementById('totalEarnings');
            const invitedUsers = document.getElementById('invitedUsers');
            const userEarnings = document.getElementById('userEarnings');
            const userProfit = document.getElementById('userProfit');
            const teamMembers = document.getElementById('teamMembers');

            // ŸÖÿ≠ÿßŸÉÿßÿ© ÿ®ŸäÿßŸÜÿßÿ™ ÿ≠Ÿäÿ©
            const onlineChange = Math.random() * 50 - 25;
            const newOnline = Math.max(10000, 12847 + onlineChange);
            onlineUsers.textContent = Math.round(newOnline).toLocaleString();
            
            const earningsChange = Math.random() * 1000 - 500;
            const newEarnings = Math.max(100000, 127430 + earningsChange);
            totalEarnings.textContent = '$' + Math.round(newEarnings).toLocaleString();
            
            const invitedChange = Math.random() * 10 - 5;
            const newInvited = Math.max(200, 287 + invitedChange);
            invitedUsers.textContent = Math.round(newInvited).toLocaleString();
            
            userEarnings.textContent = '$' + Math.round(newInvited * 3).toLocaleString();
            userProfit.textContent = '$' + Math.round(newInvited * 3).toLocaleString();
            teamMembers.textContent = Math.round(newInvited).toLocaleString();
        }

        // ÿ™ÿ≠ÿØŸäÿ´ ŸÑŸàÿ≠ÿ© ÿßŸÑÿ™ÿ≠ŸÉŸÖ
        function updateDashboard() {
            const performanceScore = document.getElementById('performanceScore');
            const goalsProgress = document.getElementById('goalsProgress');
            
            // ŸÖÿ≠ÿßŸÉÿßÿ© ÿ™ÿ∫ŸäŸäÿ±ÿßÿ™ ÿπÿ¥Ÿàÿßÿ¶Ÿäÿ© ŸÅŸä ŸÑŸàÿ≠ÿ© ÿßŸÑÿ™ÿ≠ŸÉŸÖ
            const performanceChange = Math.random() * 4 - 2;
            const newPerformance = Math.max(90, Math.min(100, 98 + performanceChange));
            performanceScore.textContent = Math.round(newPerformance) + '%';
            document.querySelector('#performanceScore + .progress-bar .progress-fill').style.width = Math.round(newPerformance) + '%';
            
            const goalsChange = Math.random() * 10 - 5;
            const newGoals = Math.max(50, Math.min(100, 75 + goalsChange));
            goalsProgress.textContent = Math.round(newGoals) + '%';
            document.querySelector('#goalsProgress + .progress-bar .progress-fill').style.width = Math.round(newGoals) + '%';
        }

        // ÿßŸÑÿ™ŸáŸäÿ¶ÿ©
        document.addEventListener('DOMContentLoaded', function() {
            createStars();
            updateCosmicClock();
            updateNetworkSpeeds();
            updateBatteryStatus();
            updateStats();
            updateDashboard();

            // ÿ™ÿ≠ÿØŸäÿ´ ÿßŸÑÿ®ŸäÿßŸÜÿßÿ™ ŸÉŸÑ ÿ´ÿßŸÜŸäÿ©
            setInterval(updateCosmicClock, 1000);
            setInterval(updateNetworkSpeeds, 3000);
            setInterval(updateBatteryStatus, 5000);
            setInterval(updateStats, 10000);
            setInterval(updateDashboard, 8000);
            
            // ÿπÿ±ÿ∂ ÿ•ÿ¥ÿπÿßÿ± ÿ™ÿ±ÿ≠Ÿäÿ®Ÿä ÿπŸÜÿØ ÿßŸÑÿ™ÿ≠ŸÖŸäŸÑ
            setTimeout(() => {
                showNotification('‚ú® ŸÖÿ±ÿ≠ÿ®Ÿãÿß ÿ®ŸÉŸÖ ŸÅŸä Cherrak Infinity - ÿßŸÑŸÖŸÜÿµÿ© ÿßŸÑÿπÿßŸÑŸÖŸäÿ© ÿßŸÑŸÖÿ™ÿ∑Ÿàÿ±ÿ©', 'success');
            }, 1000);
        });

        // ÿßŸÑÿ™ÿ£ŸÉÿØ ŸÖŸÜ ÿ≠ÿ∏ÿ± ÿ•ÿ≥ÿ±ÿßÿ¶ŸäŸÑ
        function checkCountryAccess() {
            // ŸÅŸä ÿßŸÑÿ™ÿ∑ÿ®ŸäŸÇ ÿßŸÑÿ≠ŸÇŸäŸÇŸäÿå Ÿáÿ∞ÿß ÿ≥Ÿäÿ™ÿ≠ŸÇŸÇ ŸÖŸÜ IP ÿßŸÑŸÖÿ≥ÿ™ÿÆÿØŸÖ
            const userCountry = 'DZ'; // ŸÖÿ≠ÿßŸÉÿßÿ© - ÿßŸÑÿ¨ÿ≤ÿßÿ¶ÿ±
            if (userCountry === 'IL') {
                showNotification('üö´ Ÿáÿ∞ÿß ÿßŸÑÿ®ŸÑÿØ ŸÖÿ≠ÿ∏Ÿàÿ± ŸÖŸÜ ÿßŸÑÿØÿÆŸàŸÑ', 'error');
                return false;
            }
            return true;
        }

        // ÿßŸÑÿ™ÿ≠ŸÇŸÇ ÿπŸÜÿØ ÿßŸÑÿ™ÿ≠ŸÖŸäŸÑ
        checkCountryAccess();
    </script>
